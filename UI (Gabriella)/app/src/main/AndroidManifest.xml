<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pab.ui">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity android:name=".SplashAwal" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Login"
            android:label="@string/title_activity_login" />

        <activity
            android:name=".Katalog"
            android:label="@string/title_activity_katalog" />

        <activity
            android:name=".Registrasi"
            android:label="@string/title_activity_registrasi" />

        <activity android:name=".Jual" android:label="@string/title_activity_jual" />
        <activity android:name=".Rekap" android:label="@string/title_activity_rekap" />
        <activity android:name=".Produk" android:label="@string/title_activity_produk" />
        <activity android:name=".Transaksi" android:label="@string/title_activity_transaksi" />
        <activity android:name=".KategoriRekap" android:label="@string/title_activity_kategorirekap" />
        <activity android:name=".SplashKeluar" />

    </application>

</manifest>